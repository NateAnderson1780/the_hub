<h1>Register for the Hub</h1>

<%= form_for(@user) do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>

  <%= f.label :last_name %>
  <%= f.text_field :last_name %>

  <%= f.label :email_address %>
  <%= f.text_field :email_address %>

  <%= f.label :password %>
  <%= f.text_field :password %>

  <%= f.submit "Create My Account" %>
<% end %>
