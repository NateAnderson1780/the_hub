<h1>Login to Your Account</h1>

<%= form_for(:session, url: login_path) do |f| %>
  <%= f.label :email_address %>
  <%= f.text_field :email_address %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.submit "Login" %>

<% end %>

<p>Not Registered? <%= link_to "Create Account", signup_path %>
